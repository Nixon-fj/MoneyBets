        div: div(class='tns-syncing-for__content'): span 2 for
        div: div(class='tns-syncing-for__content'): span 3 for
        div: div(class='tns-syncing-for__content'): span 4 for
    div(class='ed-container')
        div(class='ed-item no-padding') 
